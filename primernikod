        // Игровые переменные
        let gameState = {
            cards: [],
            flippedCards: [],
            matchedPairs: 0,
            moves: 0,
            gameStarted: false,
            timer: 0,
            timerInterval: null,
            totalPairs: 0
        };

        // DOM элементы
        const gameBoard = document.getElementById('game-board');
        const movesElement = document.getElementById('moves');
        const timerElement = document.getElementById('timer');
        const matchesElement = document.getElementById('matches');
        const resetButton = document.getElementById('reset-btn');
        const hintButton = document.getElementById('hint-btn');
        const factPanel = document.getElementById('fact-panel');
        const factTitle = document.getElementById('fact-title');
        const factContent = document.getElementById('fact-content');
        const closeFactButton = document.getElementById('close-fact');
        const winMessage = document.getElementById('win-message');
        const winTime = document.getElementById('win-time');
        const winMoves = document.getElementById('win-moves');
        const playAgainButton = document.getElementById('play-again-btn');

        // Создание звёздного фона
        function createStars() {
            const starsContainer = document.getElementById('stars');
            for (let i = 0; i < 200; i++) {
                const star = document.createElement('div');
                star.classList.add('star');
                const size = Math.random() * 3;
                star.style.width = `${size}px`;
                star.style.height = `${size}px`;
                star.style.left = `${Math.random() * 100}%`;
                star.style.top = `${Math.random() * 100}%`;
                star.style.opacity = Math.random() * 0.7 + 0.3;
                starsContainer.appendChild(star);
            }
        }

        // Инициализация игры
        function initGame() {
            // Очищаем предыдущее состояние
            clearInterval(gameState.timerInterval);
            gameState = {
                cards: [],
                flippedCards: [],
                matchedPairs: 0,
                moves: 0,
                gameStarted: false,
                timer: 0,
                timerInterval: null,
                totalPairs: cardData.length
            };
            
            // Обновляем отображение
            movesElement.textContent = gameState.moves;
            matchesElement.textContent = gameState.matchedPairs;
            timerElement.textContent = formatTime(gameState.timer);
            
            // Создаём пары карточек
            const cardPairs = [...cardData, ...cardData];
            
            // Перемешиваем карточки
            shuffleArray(cardPairs);
            
            // Очищаем игровое поле
            gameBoard.innerHTML = '';
            
            // Создаём карточки
            cardPairs.forEach((card, index) => {
                const cardElement = document.createElement('div');
                cardElement.classList.add('card');
                cardElement.dataset.id = card.id;
                cardElement.dataset.index = index;
                
                // Лицевая сторона карточки (изображение)
                const cardFront = document.createElement('div');
                cardFront.classList.add('card-front');
                const cardImage = document.createElement('img');
                cardImage.classList.add('card-image');
                cardImage.src = card.imageUrl;
                cardImage.alt = card.name;
                cardImage.onerror = function() {
                    // Если изображение не загрузилось, показываем заглушку
                    this.src = 'https://via.placeholder.com/150x200/1a1f4d/6d9eff?text=' + encodeURIComponent(card.name);
                };
                cardFront.appendChild(cardImage);
                
                // Обратная сторона карточки (космический узор)
                const cardBack = document.createElement('div');
                cardBack.classList.add('card-back');
                const backIcon = document.createElement('i');
                backIcon.classList.add('fas', 'fa-star');
                cardBack.appendChild(backIcon);
                
                cardElement.appendChild(cardFront);
                cardElement.appendChild(cardBack);
                
                // Добавляем обработчик клика
                cardElement.addEventListener('click', () => flipCard(cardElement, card));
                
                // Сохраняем в состоянии игры
                gameState.cards.push({
                    element: cardElement,
                    id: card.id,
                    name: card.name,
                    fact: card.fact,
                    flipped: false,
                    matched: false
                });
                
                // Добавляем на игровое поле
                gameBoard.appendChild(cardElement);
            });
            
            // Закрываем панель фактов и сообщение о победе
            factPanel.classList.remove('active');
            winMessage.classList.remove('active');
        }

        // Перемешивание массива (алгоритм Фишера-Йетса)
        function shuffleArray(array) {
            for (let i = array.length - 1; i > 0; i--) {
                const j = Math.floor(Math.random() * (i + 1));
                [array[i], array[j]] = [array[j], array[i]];
            }
            return array;
        }

        // Форматирование времени в мм:сс
        function formatTime(seconds) {
            const mins = Math.floor(seconds / 60).toString().padStart(2, '0');
            const secs = (seconds % 60).toString().padStart(2, '0');
            return `${mins}:${secs}`;
        }

        // Запуск таймера
        function startTimer() {
            if (!gameState.gameStarted) {
                gameState.gameStarted = true;
                gameState.timerInterval = setInterval(() => {
                    gameState.timer++;
                    timerElement.textContent = formatTime(gameState.timer);
                }, 1000);
            }
        }

        // Переворот карточки
        function flipCard(cardElement, cardData) {
            // Если игра не начата, начинаем
            if (!gameState.gameStarted) {
                startTimer();
            }
            
            const card = gameState.cards.find(c => c.element === cardElement);
            
            // Если карточка уже перевернута или совпала, выходим
            if (card.flipped || card.matched || gameState.flippedCards.length >= 2) {
                return;
            }
            
            // Переворачиваем карточку
            cardElement.classList.add('flipped');
            card.flipped = true;
            gameState.flippedCards.push(card);
            
            // Если перевернуты две карточки
            if (gameState.flippedCards.length === 2) {
                // Увеличиваем счетчик ходов
                gameState.moves++;
                movesElement.textContent = gameState.moves;
                
                // Проверяем, совпадают ли карточки
                const [card1, card2] = gameState.flippedCards;
                
                if (card1.id === card2.id) {
                    // Найдена пара
                    setTimeout(() => {
                        card1.element.classList.add('matched');
                        card2.element.classList.add('matched');
                        card1.matched = true;
                        card2.matched = true;
                        
                        gameState.matchedPairs++;
                        matchesElement.textContent = gameState.matchedPairs;
                        
                        // Показываем факт о найденной паре
                        showFact(card1.name, card1.fact);
                        
                        // Очищаем перевернутые карточки
                        gameState.flippedCards = [];
                        
                        // Проверяем, завершена ли игра
                        if (gameState.matchedPairs === gameState.totalPairs) {
                            endGame();
                        }
                    }, 500);
                } else {
                    // Карточки не совпадают
                    setTimeout(() => {
                        card1.element.classList.remove('flipped');
                        card2.element.classList.remove('flipped');
                        card1.flipped = false;
                        card2.flipped = false;
                        gameState.flippedCards = [];
                    }, 1000);
                }
            }
        }

        // Показ факта о планете/созвездии
        function showFact(title, content) {
            factTitle.textContent = title;
            factContent.textContent = content;
            factPanel.classList.add('active');
        }

        // Завершение игры
        function endGame() {
            clearInterval(gameState.timerInterval);
            
            // Показываем статистику победы
            winTime.textContent = formatTime(gameState.timer);
            winMoves.textContent = gameState.moves;
            
            // Показываем сообщение о победе с небольшой задержкой
            setTimeout(() => {
                winMessage.classList.add('active');
            }, 1000);
        }

        // Подсказка (временно показывает все карточки)
        function showHint() {
            if (gameState.moves === 0) return;
            
            // Переворачиваем все карточки
            gameState.cards.forEach(card => {
                if (!card.matched) {
                    card.element.classList.add('flipped');
                    card.flipped = true;
                }
            });
            
            // Через 2 секунды скрываем неподходящие карточки
            setTimeout(() => {
                gameState.cards.forEach(card => {
                    if (!card.matched) {
                        card.element.classList.remove('flipped');
                        card.flipped = false;
                    }
                });
                // Очищаем перевернутые карточки
                gameState.flippedCards = [];
            }, 2000);
        }

        // Обработчики событий
        resetButton.addEventListener('click', initGame);
        
        hintButton.addEventListener('click', showHint);
        
        closeFactButton.addEventListener('click', () => {
            factPanel.classList.remove('active');
        });
        
        playAgainButton.addEventListener('click', () => {
            winMessage.classList.remove('active');
            initGame();
        });

        // Запускаем игру при загрузке страницы
        document.addEventListener('DOMContentLoaded', () => {
            createStars();
            initGame();
        });
    </script>
</body>
</html>
